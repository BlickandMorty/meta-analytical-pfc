cpdef inline bytes encode_int64_big_endian(int64 value):
    """Decode an 64-bit signed integer from a buffer
    encoded in big-endian
    """
    cdef char* buffer = [
        (value >> 56) & 0xff,
        (value >> 48) & 0xff,
        (value >> 40) & 0xff,
        (value >> 32) & 0xff,
        (value >> 24) & 0xff,
        (value >> 16) & 0xff,
        (value >> 8) & 0xff,
        (value >> 0) & 0xff
    ]
    return <bytes>buffer[:8]


