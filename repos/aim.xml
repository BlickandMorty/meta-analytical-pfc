  .BaseTable__row:hover {
    .ActionModal__tableRowWithAction {
      &__deleteButton {
        display: flex;
      }
    }
  }
  &__tableRowWithAction {
    display: flex;
    align-items: center;
    width: 100%;
    justify-content: space-between;
