      padding-right: 2.875rem;
      &.in-progress {
        &::after {
          content: '';
          position: absolute;
          right: 1.75rem;
          width: 0.375rem;
          height: 0.375rem;
          top: 50%;
          border-radius: $border-radius-circle;
          background: $success-color-100;
          transform: translate(-50%, -50%);
        }
      }
    }
