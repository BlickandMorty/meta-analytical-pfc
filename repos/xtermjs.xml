  x=$(echo "s(2*${pi}*${y}/${period})*${amplitude}+2*${amplitude}+0.5" | bc -l)
  echo -ne "!${x%%.*}?${sixels[$((y%6))]}"
  (( y++ ))
done
echo -e "\x1b\\"
